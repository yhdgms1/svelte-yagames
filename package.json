{
  "name": "svelte-yagames",
  "version": "0.3.7",
  "type": "module",
  "author": {
    "name": "Artemiy Schukin",
    "url": "https://github.com/yhdgms1"
  },
  "contributors": [
    {
      "name": "Artemiy Schukin",
      "email": "yhdgms1@gmail.com",
      "url": "https://github.com/yhdgms1"
    }
  ],
  "bugs": {
    "url": "https://github.com/yhdgms1/svelte-yagames/issues"
  },
  "homepage": "https://github.com/yhdgms1/svelte-yagames",
  "license": "ISC",
  "scripts": {
    "build": "svelte-package",
    "preview": "vite preview"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "svelte": "./dist/index.js"
    },
    "./actions": {
      "types": "./dist/actions/index.d.ts",
      "import": "./dist/actions/index.js",
      "svelte": "./dist/actions/index.js"
    },
    "./utilities": {
      "types": "./dist/utilities/index.d.ts",
      "import": "./dist/utilities/index.js",
      "svelte": "./dist/utilities/index.js"
    },
    "./reset.css": {
      "import": "./dist/reset.css",
      "require": "./dist/reset.css",
      "svelte": "./dist/reset.css"
    },
    "./*": "./dist/*",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": { "*": ["dist/*"] }
  },
  "devDependencies": {
    "@sveltejs/package": "^2.2.1",
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "@tsconfig/svelte": "^4.0.1",
    "tslib": "^2.6.1",
    "tsup": "^6.7.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "copy-text-to-clipboard": "^3.2.0",
    "svelte": "^4.2.0"
  },
  "peerDependencies": {
    "svelte": "*"
  }
}
